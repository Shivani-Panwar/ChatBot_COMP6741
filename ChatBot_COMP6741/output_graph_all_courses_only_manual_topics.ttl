focudata:GEOG_620 a focu:Course ;
    rdf:subject "GEOG" ;
    rdf:value "620" ;
    rdfs:comment """
 This course focuses on selected topics within the discipline. Topics vary to permit investigation of current and developing theories and research areas.""" ;
    rdfs:seeAlso <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/fasc/geog-dip.html>,
