    rdf:subject "SOEN" ;
    rdf:value "6021" ;
    rdfs:comment """
""" ;
    rdfs:seeAlso <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/encs/computer-science-courses.html> ;
    foaf:name "Software Re-engineering" .

