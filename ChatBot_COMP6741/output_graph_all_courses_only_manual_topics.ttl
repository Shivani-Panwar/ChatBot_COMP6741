    rdf:subject "SCPA" ;
    rdf:value "523" ;
    rdfs:comment """
""" ;
    rdfs:seeAlso <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/fasc/scpa-ced.html>,
        <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/fasc/scpa-dec.html> ;
