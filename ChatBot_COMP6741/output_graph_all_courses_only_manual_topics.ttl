focudata:ENGR_6131 a focu:Course ;
    focu:includes focudata:Controllability,
        focudata:Kalman_filter,
        focudata:State-space_representation,
        focudata:decomposition,
        focudata:dynamic_systems,
        focudata:modal,
        focudata:observability,
        focudata:optimal_control,
        focudata:quadratic,
        focudata:robustness ;
    rdf:subject "ENGR" ;
