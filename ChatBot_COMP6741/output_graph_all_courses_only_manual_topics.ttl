focudata:CATS_691 a focu:Course ;
    rdf:subject "CATS" ;
    rdf:value "691" ;
    rdfs:comment """
 This course presents an overview of qualitative research, theory and methodology, and their application to the creative arts therapies. Students develop critical reading skills and explore ethical issues involved in research.""" ;
    rdfs:seeAlso <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/fofa/cats-art.html>,
        <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/fofa/cats-drama.html>,
        <https://www.concordia.ca/content/concordia/en/academics/graduate/calendar/current/fofa/cats-music.html> ;
    foaf:name "Research in the Creative Arts Therapies" .

