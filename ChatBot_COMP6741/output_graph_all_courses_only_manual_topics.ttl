    rdf:value "475" ;
    rdfs:comment "                      Men and Masculinities" ;
    rdfs:seeAlso <https://www.concordia.ca/content/concordia/en/academics/undergraduate/calendar/current/sec31/31-310.html> ;
    foaf:name "(also listed as ANTH 475)" .

focudata:SOCI_476 a focu:Course ;
    rdf:subject "SOCI" ;
    rdf:value "476" ;
    rdfs:comment """
Prerequisite: See N.B. number (2). Analysis of recent debates and perspectives within feminist social theory, in particular the concepts of gender, equality, difference, identity, and power; feminist dialogues with, and critiques of, sociological theory.""" ;
    rdfs:seeAlso <https://www.concordia.ca/content/concordia/en/academics/undergraduate/calendar/current/sec31/31-310.html> ;
    foaf:name "Feminist Sociological Theories" .

